{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "path/to/your/csvData.csv",
      "format": {
        "type": "csv"
      }
    },
    "mark": "line",
    "encoding": {
      "x": {
        "field": "year",
        "type": "ordinal",
        "axis": {"labelAngle": 0}
      },
      "y": {
        "field": "population",
        "type": "quantitative"
      },
      "color": {
        "field": "name",
        "type": "nominal"
      }
    },
    "transform": [
      {
        "filter": {"field": "name", "oneOf": ["China", "India", "United States of America"]}
      }
    ]
  }
  